<title>{{title}}</title>


{{#each items}}
    <div class="card" style="width:100%;margin:5px 20px 5px 5px;">
    <div class="card-header" style="text-align:center;">
      الاختبارات والمقاييس
    </div>
    <div class="card-body" style="width:100%;">
       <h5 class="card-title">اختبار {{this.title}}</h5>
       <p class="card-text">{{this.header}}</p>
       <a href="/pages/standard/{{this.title}}" class="btn btn-primary">قراءة</a>
    </div>
    </div>
{{/each}}
